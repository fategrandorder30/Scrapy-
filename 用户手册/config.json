[  //最外层的列表，列表里的每一个字典都表示一个要被爬取的网页，例子里目前只有一个广东省人民政府要爬
  {  //第一个例子
    "name": "广东省人民政府",  //爬取网页自定义的名称，用于标识
    "url": "https://www.gd.gov.cn/zwgk/wjk/qbwj/index.html",  //网页地址url
    "selectors": {
      "title": "//div[@class='viewList']/ul/li/span/a/text()",  //政策列表里政策对应标题的Xpath位置
      "link": "//div[@class='viewList']/ul/li/span/a/@href",  //政策列表里政策链接的Xpath位置
      "content": {  //政策链接里的信息字段，设定为字典，表示可以多个位置爬取
        "正文": "/html/body/div[3]/div[3]/div[1]/div[4]/div[2]/span/text()",  //链接中的正文字段
        "文号": "/html/body/div[3]/div[3]/div[1]/div[4]/div[2]/span/text()"  //链接中的文号字段
      },
      "next_page": "//div[@class='page']/a[contains(text(), '下一页')]/@href"  //政策列表一般不止一页，翻页按钮Xpath位置
    },
    "regex_replacements": {  //正则替换
      "title": [  //标题替换，这个列表表示可以有多个替换规则一起作用于该标题
        [  //第一个正则替换规则，里面应该有两个字符串，第一个是匹配模式，第二个是替换模式
          "[\\s\\S]+",
          "通知"
        ]
      ],
      "content": [  //爬取字段正则替换规则，列表元素表示与上面的content一一对应
        [  //第一个字段“正文”的正则规则，里面有两条替换规则
          [  //第一个替换规则
            "(\\d{4})-(\\d{2})-(\\d{2})",
            "\\1年\\2月\\3日"
          ],
          [  //第二个替换规则
            "(\\d{4})年(\\d{2})月(\\d{2})日",
            "\\1/\\2/\\3"
          ]
        ]
      ]
    }
  }
]